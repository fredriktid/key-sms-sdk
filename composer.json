{
    "name": "fredriktid/key-sms-sdk",
    "license": "MIT",
    "type": "library",
    "description": "PHP SDK for KeySMS",
    "homepage": "https://github.com/fredriktid/key-sms-sdk",
    "autoload": {
        "psr-0" : {
            "FTidemann\\KeySms" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FTidemann\\KeySms\\Tests\\" : "tests"
        }
    },
    "authors": [
        {
            "name": "Fredrik Tidemann",
            "email": "fredrik.tid@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "psr/log": "^1",
        "php-http/client-implementation": "^1.0",
        "php-http/httplug": "^1.0",
        "php-http/discovery": "^1.0",
        "php-http/message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5",
        "php-http/guzzle6-adapter": "^1.0",
        "php-http/mock-client":  "^0.3"
    }
}
